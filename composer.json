{
    "name": "rahmatwaisi/otp-auth",
    "license": "MIT",
    "description": "Easily Generate and Verify an OTP using laravel cache",
    "homepage": "https://github.com/mr648/otp-auth",
    "type": "library",
    "keywords": [
        "otp-auth",
        "otp auth",
        "otp",
        "laravel",
        "framework",
        "auth",
        "laravel package",
        "laravel auth",
        "rahmatwaisi otp",
        "laravel otp"
    ],
    "support": {
        "issues": "https://github.com/mr648/otp-auth/issues",
        "source": "https://github.com/mr648/otp-auth"
    },
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "RahmatWaisi\\OtpAuth\\": "src"
        }
    },
     "autoload-dev": {
        "psr-4": {
            "RahmatWaisi\\OtpAuth\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Rahmat Waisi",
            "email": "rahmatwaisi@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-php coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RahmatWaisi\\OtpAuth\\Providers\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "RahmatWaisi\\OtpAuth\\Facades\\OtpGenerator"
            }
        }
    }
}
