{
    "name": "rahmatwaisi/otp-auth",
    "license": "MIT",
    "description": "Easily Generate and Verify an OTP using laravel cache",
    "keywords": [
        "otp-auth",
        "otp auth",
        "otp",
        "laravel",
        "framework",
        "auth",
        "laravel package",
        "laravel auth",
        "laravel otp"
    ],

    "support": {
        "issues": "https://github.com/mr648/otp-auth/issues",
        "source": "https://github.com/mr648/otp-auth"
    },

    "autoload": {
        "psr-4": {
            "RahmatWaisi\\OtpAuth\\": "src/",
            "RahmatWaisi\\OtpAuth\\Tests\\": "tests/"
        }
    },

    "authors": [
        {
            "name": "Rahmat Waisi",
            "email": "rahmatwaisi@gmail.com"
        }
    ],

    "require": {
        "php": "^8.0.2"
    },


    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },

    "extra": {
        "laravel": {
            "providers": [
                "RahmatWaisi\\OtpAuth\\Providers\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "STS\\ZipStream\\OtpGenerator"
            }
        }
    }
}
